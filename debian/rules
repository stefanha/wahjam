#!/usr/bin/make -f
# -*- makefile -*-
# Sample debian/rules that uses debhelper.
# This file was originally written by Joey Hess and Craig Small.
# As a special exception, when this file is copied by dh-make into a
# dh-make output file, you may use that output file without restriction.
# This special exception was added by Craig Small in version 0.37 of dh-make.

# Uncomment this to turn on verbose mode.
export DH_VERBOSE=1

override_dh_auto_configure:
	set
	dh_auto_configure -- QTCLIENT_TARGET=jammr \
	                     JAMMR_API_URL="http://jammr.net/api/" \
			     JAMMR_REGISTER_URL="http://jammr.net/accounts/register/" \
			     JAMMR_UPGRADE_URL="http://jammr.net/pricing.html" \
			     JAMMR_UPDATE_URL="http://jammr.net/static/latest-linux.txt" \
			     JAMMR_DOWNLOAD_URL="http://jammr.net/download.html" \
			     VERSION=1.0.0 \
			     APPNAME=jammr \
			     ORGNAME=jammr \
			     ORGDOMAIN=jammr.net

%:
	dh $@ 
